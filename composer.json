{
    "name": "mmoreram/symfony-bundle-dependencies",
    "description": "Dependencies resolver for Symfony Bundles",
    "keywords": [
        "symfony",
        "dependencies",
        "bundle"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "symfony/http-kernel": "^3.2|^4.0|^5.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^3.2|^4.0|^5.0",
        "phpunit/phpunit": "^9",
        "phpspec/prophecy-phpunit": "^2",
        "friendsofphp/php-cs-fixer": "^2.0",
        "mmoreram/php-formatter": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Mmoreram\\SymfonyBundleDependencies\\": ""
        }
    }
}
